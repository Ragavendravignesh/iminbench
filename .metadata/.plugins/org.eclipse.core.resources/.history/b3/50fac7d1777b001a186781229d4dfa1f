spring.application.name=Zuulserver1
server.port=8080

zuul.routes.login.path=/loginsignup
zuul.routes.login.strip-prefix=false
zuul.routes.login.service-id=LoginSignUpMS

zuul.routes.fresher.path=/fresher
zuul.routes.fresher.strip-prefix=false
zuul.routes.fresher.service-id=FresherMS

zuul.routes.project.path=/project
zuul.routes.project.strip-prefix=false
zuul.routes.project.service-id=ProjectMS

eureka.client.service-url.defaultZone=http://localhost:5555/eureka
